@model List<ExchangeViewModel.Exchange_Rates>
@{
    Layout = null;
}

<link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />

<table class="table table-hover">

    <tr>
        <td>Döviz</td>
        <td>Miktar</td>
    </tr>

    @foreach(var item in Model)
    {
        <tr>
            <td>@item.currency</td>
            <td>@item.exchange_rate_buy</td>
        </tr>
    }

</table>

